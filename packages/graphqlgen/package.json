{
  "name": "graphqlgen",
  "version": "0.4.0",
  "description": "Generate resolver types based on a GraphQL Schema",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "graphql-resolver-codegen": "dist/index.js",
    "graphqlgen": "dist/index.js",
    "gg": "dist/index.js"
  },
  "scripts": {
    "prepublish": "rm -rf example/node_modules && npm test && npm run build",
    "bench": "ts-node benchmarks/index.ts",
    "build": "npm run lint && tsc --declaration",
    "watch": "tsc -w",
    "lint": "tslint --project tsconfig.json {src,test}/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run lint && jest --maxWorkers 4",
    "gen": "ts-node --files src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prisma/graphqlgen.git"
  },
  "author": "Prisma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prisma/graphqlgen/issues"
  },
  "homepage": "https://github.com/prisma/graphqlgen#readme",
  "dependencies": {
    "@babel/parser": "^7.1.5",
    "@babel/types": "7.2.2",
    "ajv": "^6.5.5",
    "camelcase": "5.0.0",
    "chalk": "2.4.1",
    "glob": "^7.1.3",
    "graphql": "^0.13.0 || ^14.0.0",
    "graphql-import": "0.7.1",
    "graphqlgen-json-schema": "0.2.12",
    "js-yaml": "3.12.0",
    "mkdirp": "0.5.1",
    "prettier": "1.15.3",
    "reason": "3.3.4",
    "rimraf": "2.6.2",
    "typescript": "3.2.2",
    "yargs": "12.0.5"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/callsite": "^1.0.30",
    "@types/camelcase": "4.1.0",
    "@types/graphql": "14.0.3",
    "@types/jest": "23.3.10",
    "@types/js-yaml": "3.11.4",
    "@types/mkdirp": "0.5.2",
    "@types/node": "10.12.18",
    "@types/prettier": "1.15.2",
    "@types/rimraf": "2.0.2",
    "@types/yargs": "12.0.1",
    "benchmark": "^2.1.4",
    "callsite": "^1.0.0",
    "flow-bin": "0.86.0",
    "jest": "23.6.0",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.12.0"
  }
}
